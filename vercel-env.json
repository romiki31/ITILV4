{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "redirects": [
    {
      "source": "/(.*)",
      "destination": "/maintenance.html?maintenance=true",
      "permanent": false,
      "has": [
        {
          "type": "header",
          "key": "x-maintenance-mode",
          "value": "true"
        }
      ]
    }
  ]
}